class Breakfast {
  init(meat, bread) {
    this.meat = meat;
    this.bread = bread;
  }

  cook() {
    print "Eggs a-fryin'!";
  }

  serve(who) {
    print "Enjoy your breakfast, " + who + ".";
  }
}

// Here is a comment
var baconAndToast = Breakfast("bacon", "toast");
baconAndToast.serve("Dear Reader");

